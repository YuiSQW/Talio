<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.TilePane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="529.0" prefWidth="671.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.scenes.BoardOverviewCtrl">
   <children>
      <TextField layoutX="29.0" layoutY="35.0" prefHeight="42.0" prefWidth="174.0" text="Board title">
         <font>
            <Font size="15.0" />
         </font>
      </TextField>
      <Pane fx:id="toolBar" prefHeight="26.0" prefWidth="686.0">
         <children>
            <Button fx:id="closeButton" layoutX="645.0" layoutY="6.0" mnemonicParsing="false" onAction="#close" prefHeight="25.0" prefWidth="22.0" style="-fx-background-radius: 1em;" text="x" />
            <Button fx:id="minimizeButton" layoutX="614.0" layoutY="6.0" mnemonicParsing="false" onAction="#minimize" prefHeight="25.0" prefWidth="23.0" style="-fx-background-radius: 1em;" text="-" />
         </children>
      </Pane>
      <ScrollPane layoutX="2.0" layoutY="106.0" prefHeight="422.0" prefWidth="686.0">
         <content>
            <TilePane fx:id="tilePane" hgap="50.0" prefHeight="422.0" prefWidth="686.0">
               <children>
                  <VBox fx:id="v1" prefHeight="360.0" prefWidth="180.0">
                     <children>
                        <Button fx:id="addList" minHeight="-Infinity" minWidth="35.0" mnemonicParsing="false" onAction="#addNewList" prefHeight="41.0" prefWidth="174.0" style="-fx-background-radius: 100;" text="+ Add another list" textFill="#080808" />
                     </children>
                  </VBox>
               </children>
               <padding>
                  <Insets bottom="10.0" left="20.0" right="10.0" top="10.0" />
               </padding>
            </TilePane>
         </content>
      </ScrollPane>

   </children>
</AnchorPane>
